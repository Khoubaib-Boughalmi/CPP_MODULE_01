main = sed
CC = c++
CFLAGS = -std=c++98 -Wall -Werror -Wextra

SRCS =	main.cpp main main

%.o:%.cpp
		${CC} ${CFLAGS} -c $^ -o $@

OBJS = ${SRCS:.cpp=.o}

${main} :	${OBJS}
			${CC} ${main} ${OBJS} -o ${main}

all : ${main}

clean:	
		${RM} ${OBJS} 
	
fclean:	clean
		${RM} ${main}
