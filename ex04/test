NAME = sed
CC = c++
CFLAGS = -std=c++98 -Wall -Werror -Wextra

SRCS =	main.cpp main main

%.o:%.cpp
		${CC} ${CFLAGS} -c $^ -o $@

OBJS = ${SRCS:.cpp=.o}

${NAME} :	${OBJS}
			${CC} ${main} ${OBJS} -o ${NAME}

all : ${NAME}

clean:	
		${RM} ${OBJS} 
	
fclean:	clean
		${RM} ${NAME}
